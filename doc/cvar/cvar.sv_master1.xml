<refentry id="cvar.sv_master1">

<refmeta>
    <refentrytitle>sv_master1</refentrytitle>
    <manvolnum>cvar</manvolnum>
</refmeta>

<refnamediv>
    <refname>sv_master1</refname>
    <refpurpose>set master server for browse-list registration</refpurpose>
</refnamediv>

<refsynopsisdiv>
    <cmdsynopsis>
        <command>sv_master1</command>
        <arg><replaceable>"hostname"</replaceable></arg>
    </cmdsynopsis>
</refsynopsisdiv>

<refsection>
<title>Default</title>
    <cmdsynopsis>
        <command>sv_master1</command>
        <arg choice="plain"><literal>"etmaster.idsoftware.com"</literal></arg>
    </cmdsynopsis>
</refsection>

<refsection>
<title>Description</title>
<para>
    <command>sv_master1</command>
    sets the master server for browse-list registration.
</para>
</refsection>

