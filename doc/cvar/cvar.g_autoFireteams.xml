<refentry id="cvar.g_autoFireteams">

<refmeta>
    <refentrytitle>g_autoFireteams</refentrytitle>
    <manvolnum>cvar</manvolnum>
</refmeta>

<refnamediv>
    <refname>g_autoFireteams</refname>
    <refpurpose>enables/disables automatic fireteam placement</refpurpose>
</refnamediv>

<refsynopsisdiv>
    <cmdsynopsis>
        <command>g_autoFireteams</command>
        <arg><replaceable>mode</replaceable></arg>
    </cmdsynopsis>
    &boolean.mode;
</refsynopsisdiv>

<refsection>
<title>Default</title>
    <cmdsynopsis>
        <command>g_autoFireteams</command>
        <arg choice="plain"><literal>0</literal></arg>
    </cmdsynopsis>
</refsection>

<refsection>
<title>Description</title>
<para>
    <command>g_autoFireteams</command>
    enables or disables automatic fireteam placement.
</para>
</refsection>

</refentry>
